
@{
    ViewBag.Title = "DashBoard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html lang="en">

<head>
    
    <!-- CSS LINK -->
    <link rel="stylesheet" href="~/Content/addcss/StyleSheet.css">

    <!-- Responsive css-->
    <link rel="stylesheet" href="~/Content/addcss/responsive.css">
</head>

<body>
    <section id="dashboard">
        <div class="container">
            <!-- Logo -->
            <a href="#user" class="navbar-brand smooth-scroll">
                <img src="~/images/home/logo.png" alt="top logo" class="topi" style="border-radius:0%!important">
            </a>

            <div class="container">
                <div class="container">
                    <nav class="navbar" style="position: absolute">
                        <div class="navbar-fixed-top">
                            <!-- Mobile Menu Open Button -->
                            <span id="mobile-nav-open-btn">&#9776;</span>
                            <!-- Logo -->
                            <!-- Mobile Menu -->
                            <div id="mobile-nav">
                                <!-- open -->
                                <span id="mobile-nav-close-btn">&times;</span>

                                <div id="mobile-nav-content">
                                    <ul class="nav">
                                        <li><a class="smooth-scroll" href="/Admin/Dashboard">Dashboard</a></li>
                                        <li><a class="smooth-scroll" href="/Admin/UnderReview">Notes</a></li>
                                        <li><a class="smooth-scroll" href="/Admin/Members">Members</a></li>
                                        <li><a class="smooth-scroll" href="/Auth/FAQ">FAQ</a></li>
                                        <li><a class="smooth-scroll" href="#contactUs">Rports</a></li>
                                        <li><a class="smooth-scroll" href="#contactUs">Setting</a></li>
                                        <li>
                                            <div class="dropdown">
                                                <img class="smooth-scroll" style="margin-top: 10px;" src="~/images/Dashboard/user-img.png" alt="img" class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                    <a class="dropdown-item" href="#">Upload Profile</a><br>
                                                    <a class="dropdown-item" href="#">Change password</a><br>
                                                    <a class="dropdown-item" href="#">Log Out</a><br>
                                                </div>
                                            </div>
                                        </li>
                                        <li>
                                            <a class="btn1">
                                                Logout
                                            </a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </nav>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav pull-right">
                            <li><a class="smooth-scroll" href="/Admin/Dashboard" style="color: black;">Dashboard</a></li>
                            <li><a class="smooth-scroll" href="/Admin/UnderReview" style="color: black;">Notes</a></li>
                            <li><a class="smooth-scroll" href="/Admin/Members" style="color: black;">Members</a></li>
                            <li><a class="smooth-scroll" href="/Auth/FAQ" style="color: black;">FAQ</a></li>
                            <li><a class="smooth-scroll" href="#contactUs" style="color: black;">Rports</a></li>
                            <li><a class="smooth-scroll" href="#contactUs" style="color: black;">Setting</a></li>
                            <li>
                                <div class="dropdown">
                                    <img class="smooth-scroll" style="margin-top: 10px;" src="~/images/Dashboard/user-img.png" alt="img" class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                        <a class="dropdown-item" href="/Auth/UserProfile">Upload Profile</a><br>
                                        <a class="dropdown-item" href="/Auth/ChangePass">Change password</a><br>
                                        <a class="dropdown-item" href="#">Log Out</a><br>
                                    </div>
                                </div>
                            </li>
                            <li>
                                <a class="btn1">
                                    Logout
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="fancy-line" style="margin-bottom: 20px;">
            <hr class="line1">
        </div>
        <div class="dash-1">
            <h1>Dashboard</h1>
        </div>
        <div class="dash1">
            <div class="container" style="margin-top: 30px;">
                <div class="row">
                    <div class="col-md-3 col-sm-4 dashbo">
                        <h3>02</h3>
                        <p>Numbers of notes Reveiew for Publish</p>
                    </div>
                    <div class="col-md-3 col-sm-4 dashbo">
                        <h3>01</h3>
                        <p>Numbers of notes Downloaded</p>
                        <p>(last 7 days)</p>
                    </div><div class="col-md-3 col-sm-4 dashbo">
                        <h3>01</h3>
                        <p>Numbers of notes Registrations</p>
                        <p> (Last 7 days)</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="dash5">
            <div class="container" style="margin-top: 60px;">
                <div class="row">
                    <div class="col-md-6">
                        <h4>Publised Notes</h4>
                    </div>
                    <div class="col-md-6" style="margin-top: 20px;">
                        <div class="col-md-4">
                            <input type="search" placeholder="Search" class="searchb">
                            <i toggle="#search" class="fa fa-search field-icon1"></i>
                        </div>
                        <div class="col-md-4">
                            <a class="btn11">SEARCH</a>
                        </div>
                        <div class="col-md-4">
                            <div class="dropdown">
                                <a type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" class="btn-drop">
                                    SELECT MONTH
                                </a>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">1</a><br>
                                    <a class="dropdown-item" href="#">2</a>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
        <div class="container" style="margin-top: 20px">
            <div class="table-responsive">
                <table class="table">
                    <tbody>
                        <tr>
                            <th scope="col">SR NO.</th>
                            <th scope="col">TITLE</th>
                            <th scope="col">CATEGORY</th>
                            <th scope="col">ATTACHEMENT</th>
                            <th scope="col">SELL TYPE</th>
                            <th scope="col">PRICE</th>
                            <th scope="col">PUBLISH</th>
                            <th scope="col">PUBLISHED DATE</th>
                            <th scope="col">NUMBER OF<br>DOWNLOADS</th>
                            <th scope="col"> </th>
                        </tr>
                    </tbody>
                    <tbody>
                        @foreach (NoteMarket.Models.NoteDetail note1 in Model)
                        {
                            if (note1.Status == "Published")
                            {
                            <tr>
                            <th scope="row">1</th>
                            <td>@note1.NoteTitle</td>
                            <td>@note1.Category</td>
                            <td>10 KB</td>
                            <td>
                                @if (@note1.SellFor == true)
                                {
                                    <p>Paid</p>
                                }
                                @if (@note1.SellFor == false)
                                 {
                                     <p>Free</p>
                                  }
                             </td>
                            <td>$@note1.SellPrice</td>
                            <td>@if (note1.OwnerId == 11) { <p>Dhruvinsinh</p>}
                                @if (note1.OwnerId == 10) { <p>mira</p>}
                                </td>
                            <td>@note1.ApprovedDate</td>
                            <td>10</td>
                            <td>
                            <div class="dropdown">
                                <i class="fa fa-ellipsis-v" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                </i>
                                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item1" href="#">Download Notes</a><br>
                                    <a class="dropdown-item1" href="#">View More Details</a><br>
                                    <a class="dropdown-item1" href="#">Unpblished</a><br>
                                </div>
                            </div>
                            </td>
                            </tr>}
                        
                            }
                        
                    </tbody>
                </table>
            </div>
            <ul class="pagination">
                <li class="page-item">
                    <a class="page-link" href="#" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a>
                </li>
                <li class="page-item"><a class="page-link btn-pagination" href="#">1</a></li>
                <li class="page-item"><a class="page-link" href="#">2</a></li>
                <li class="page-item"><a class="page-link" href="#">3</a></li>
                <li class="page-item"><a class="page-link" href="#">4</a></li>
                <li class="page-item"><a class="page-link" href="#">5</a></li>
                <li class="page-item">
                    <a class="page-link" href="#" aria-label="Next">
                        <span aria-hidden="true">&raquo;</span>
                    </a>
                </li>
            </ul>
            <div class="fancy-line" style="margin-bottom: 20px;">
                <hr class="line1">
            </div>
            <div class="row">
                <div class="col-md-6">
                    <p>
                        Version:1.1.24
                    </p>
                </div>
                <div class="col-md-6">
                    <p style="text-align: end">
                        Copyright &copy; Tatvasoft Allrights Reserved
                    </p>
                </div>

            </div>
        </div>

    </section>
    <!-- jquery -->
    <script src="js/jquery.js"></script>
    <!--bootstrap js -->
    <script src="js/bootstrep/bootstrap.js"></script>
    <!-- custom js file -->
    <script src="js/script.js"></script>
</body>
</html>