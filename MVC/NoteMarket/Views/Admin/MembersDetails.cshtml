@model NoteMarket.Models.MemberDetailfull
@{
    IEnumerable<NoteMarket.Models.NoteDetail> noteDetails = ViewData["Notedetail"] as IEnumerable<NoteMarket.Models.NoteDetail>;
    ViewBag.Title = "MembersDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!-- CSS LINK -->
<link rel="stylesheet" href="~/Content/addcss/StyleSheet.css">

<!-- Responsive css-->
<link rel="stylesheet" href="~/Content/addcss/responsive.css">
<section id="memberdetail">
    <div class="container">
        <!-- Logo -->
        <a href="#user" class="navbar-brand smooth-scroll">
            <img src="~/images/home/logo.png" alt="top logo" class="topi" style="border-radius:0%!important">
        </a>

        <div class="container">
            <div class="container">
                <nav class="navbar" style="position: absolute">
                    <div class="navbar-fixed-top">
                        <!-- Mobile Menu Open Button -->
                        <span id="mobile-nav-open-btn">&#9776;</span>
                        <!-- Logo -->
                        <!-- Mobile Menu -->
                        <div id="mobile-nav">
                            <!-- open -->
                            <span id="mobile-nav-close-btn">&times;</span>

                            <div id="mobile-nav-content">
                                <ul class="nav">
                                    <li><a class="smooth-scroll" href="#SearchNotes">Dashboard</a></li>
                                    <li><a class="smooth-scroll" href="#SellYourNotes">Notes</a></li>
                                    <li><a class="smooth-scroll" href="#BuyerRequests">Members</a></li>
                                    <li><a class="smooth-scroll" href="#FAQ">FAQ</a></li>
                                    <li><a class="smooth-scroll" href="#contactUs">Rports</a></li>
                                    <li><a class="smooth-scroll" href="#contactUs">Setting</a></li>
                                    <li>
                                        <div class="dropdown">
                                            <img class="smooth-scroll" style="margin-top: 10px;" src="images/Dashboard/user-img.png" alt="img" class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                <a class="dropdown-item" href="#">Upload Profile</a><br>
                                                <a class="dropdown-item" href="#">Change password</a><br>
                                                <a class="dropdown-item" href="#">Log Out</a><br>
                                            </div>
                                        </div>
                                    </li>
                                    <li>
                                        <a class="btn1">
                                            Logout
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </nav>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav pull-right">
                        <li><a class="smooth-scroll" href="#SearchNotes" style="color: black">Dashboard</a></li>
                        <li><a class="smooth-scroll" href="#SellYourNotes" style="color: black">Notes</a></li>
                        <li><a class="smooth-scroll" href="#BuyerRequests" style="color: black">Members</a></li>
                        <li><a class="smooth-scroll" href="#FAQ" style="color: black">FAQ</a></li>
                        <li><a class="smooth-scroll" href="#contactUs" style="color: black">Rports</a></li>
                        <li><a class="smooth-scroll" href="#contactUs" style="color: black">Setting</a></li>
                        <li>
                            <div class="dropdown">
                                <img class="smooth-scroll" style="margin-top: 10px;" src="images/Dashboard/user-img.png" alt="img" class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                    <a class="dropdown-item" href="#">Upload Profile</a><br>
                                    <a class="dropdown-item" href="#">Change password</a><br>
                                    <a class="dropdown-item" href="#">Log Out</a><br>
                                </div>
                            </div>
                        </li>
                        <li>
                            <a class="btn1">
                                Logout
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
    <div class="fancy-line" style="margin-bottom: 20px;">
        <hr class="line1">
    </div>
    <div class="container">
        <h1 style="text-align: left; font-size: 28px">Member Details</h1>
        <div class="row">
            <div class="col-md-3">
                <img src=@Model.img.Substring(1) alt="img" style=" height:200px;width:100%;margin-top:5px;border-radius:0%">
            </div>
            <div class="col-md-2">
                <h4>First Name:</h4>
                <h4>Last Name:</h4>
                <h4>Email:</h4>
                <h4>DOB:</h4>
                <h4>Phone Number:</h4>
                <h4>Collage/University:</h4>
            </div>
            <div class="rb">
                <div class="col-md-2">
                    <h5>@Model.fn</h5>
                    <h5>@Model.ln</h5>
                    <h5>@Model.emailid</h5>
                    <h5>@Model.bd</h5>
                    <h5>@Model.phone</h5>
                    <h5>@Model.uni</h5>
                </div>
            </div>
            <div class="col-md-1">
                <div class="vl"></div>
            </div>
            <div class="col-md-2 ">
                <h4>Address1:</h4>
                <h4>Address2:</h4>
                <h4>City:</h4>
                <h4>State:</h4>
                <h4>Country:</h4>
                <h4>Zip Code:</h4>
            </div>
            <div class="rb">
                <div class="col-md-2 ">
                    <h5>@Model.ad1</h5>
                    <h5>@Model.ad2</h5>
                    <h5>@Model.city</h5>
                    <h5>@Model.state</h5>
                    <h5>@Model.country</h5>
                    <h5>@Model.zip</h5>
                </div>
            </div>
        </div>
        <h1 style="font-size: 25px;text-align: left">Notes</h1>
        <div class="container" style="margin-top: 20px">
            <div class="table-responsive">
                <table class="table">
                    <tbody>
                        <tr>
                            <th scope="col">SR NO.</th>
                            <th scope="col">NOTETITEL</th>
                            <th scope="col">CATEGORY</th>
                            <th scope="col">STATUS</th>
                            <th scope="col">DOWNLOAD NOTES</th>
                            <th scope="col">TOTAL EARNING</th>
                            <th scope="col">DATE ADDED</th>
                            <th scope="col">PUBLISHED DATE</th>
                            <th scope="col"></th>
                        </tr>
                    </tbody>
                    <tbody>

                        @foreach (NoteMarket.Models.NoteDetail note1 in noteDetails)
                        {
                            if (note1.OwnerId == Model.id)
                            {
                            <tr>
                                <th scope="col">1</th>
                                <th scope="col" style="color: #6255a5">@note1.NoteTitle</th>
                                <th scope="col">@note1.Category</th>
                                <th scope="col">@note1.Status</th>
                                <th scope="col">22</th>
                                <th scope="col">$177</th>
                                <th scope="col">@note1.ReqDate</th>
                                <th scope="col">@note1.ApprovedDate</th>
                                <th scope="col">
                                    <div class="dropdown">
                                        <i class="fa fa-ellipsis-v" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                        </i>
                                        <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                            <a class="dropdown-item1" href="#">Download Notes</a><br>

                                        </div>
                                    </div>
                                </th>
                            </tr>}
                        }
                       
</table>
            </div>
        </div>
    </div>
    <ul class="pagination">
        <li class="page-item">
            <a class="page-link" href="#" aria-label="Previous">
                <span aria-hidden="true">&laquo;</span>
            </a>
        </li>
        <li class="page-item"><a class="page-link btn-pagination" href="#">1</a></li>
        <li class="page-item"><a class="page-link" href="#">2</a></li>
        <li class="page-item"><a class="page-link" href="#">3</a></li>
        <li class="page-item"><a class="page-link" href="#">4</a></li>
        <li class="page-item"><a class="page-link" href="#">5</a></li>
        <li class="page-item">
            <a class="page-link" href="#" aria-label="Next">
                <span aria-hidden="true">&raquo;</span>
            </a>
        </li>
    </ul>
    <div class="fancy-line" style="margin-bottom: 20px;">
        <hr class="line1">
    </div>
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-6">
                    <p>
                        Version:1.1.24
                    </p>
                </div>
                <div class="col-md-6">
                    <p style="text-align: end">
                        Copyright &copy; Tatvasoft Allrights Reserved
                    </p>
                </div>
            </div>
        </div>
    </footer>
</section>

